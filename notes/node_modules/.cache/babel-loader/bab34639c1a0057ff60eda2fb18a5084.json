AA,EAAA,cAAA;AAHA,OAXA;AATA,KAAA;AA2BA,GApCA;AAqCA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AAAA,uBACA,KAAA,IADA;AAAA,UACA,KADA,cACA,KADA;AAAA,UACA,WADA,cACA,WADA;;AAEA,UAAA,KAAA,KAAA,EAAA,IAAA,WAAA,KAAA,EAAA,EAAA;AACA,aAAA,OAAA,GAAA,6BAAA;AACA,eAAA,KAAA;AACA;;AACA,WAAA,KAAA,CAAA,IAAA,CAAA;AACA,QAAA,KAAA,EAAA,KADA;AAEA,QAAA,WAAA,EAAA,WAFA;AAGA,QAAA,IAAA,EAAA,IAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,EAAA,cAAA;AAHA,OAAA;AAKA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,WAAA,GAAA,EAAA;AACA,KAfA;AAgBA,IAAA,UAhBA,sBAgBA,KAhBA,EAgBA;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AAlBA,GArCA;AAyDA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,UAAA,KAAA,GAAA,KAAA,KAAA;AAAA,UACA,MAAA,GAAA,KAAA,MADA;AAEA,UAAA,CAAA,MAAA,EAAA,OAAA,KAAA,CAHA,CAIA;;AACA,MAAA,MAAA,GAAA,MAAA,CAAA,IAAA,GAAA,WAAA,EAAA,CALA,CAMA;;AACA,MAAA,KAAA,GAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,KAAA,CAAA,WAAA,GAAA,OAAA,CAAA,MAAA,MAAA,CAAA,CAAA,EAAA;AACA,iBAAA,IAAA;AACA;AACA,OAJA,CAAA,CAPA,CAYA;;AACA,aAAA,KAAA;AACA;AAfA;AAzDA,CAAA","sourcesContent":["<template>\n    <div class=\"wrapper\">\n      <div class=\"wrapper-content\">\n\n        <section>\n          <div class=\"container\">\n            <!-- Error message -->\n            <Message v-if=\"message\" :message=\"message\"/>\n\n            <!-- Add note -->\n            <NewNote :note='note' @addNote=\"addNote\"/>\n\n            <div class=\"note-header\">\n              <h1>{{title}}</h1>\n\n              <Search :value=\"search\" placeholder=\"Find note\" @search=\"search = $event\"/>\n\n              <!-- icons controls -->\n              <div class=\"icons\">\n                <svg :class=\"{ active: grid}\" @click=\"grid = true\"  xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" ><rect x=\"3\" y=\"3\" width=\"7\" height=\"7\"></rect><rect x=\"14\" y=\"3\" width=\"7\" height=\"7\"></rect><rect x=\"14\" y=\"14\" width=\"7\" height=\"7\"></rect><rect x=\"3\" y=\"14\" width=\"7\" height=\"7\"></rect></svg>\n                <svg :class=\"{ active: !grid }\" @click=\"grid = false\" xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"><line x1=\"8\" y1=\"6\" x2=\"21\" y2=\"6\"></line><line x1=\"8\" y1=\"12\" x2=\"21\" y2=\"12\"></line><line x1=\"8\" y1=\"18\" x2=\"21\" y2=\"18\"></line><line x1=\"3\" y1=\"6\" x2=\"3\" y2=\"6\"></line><line x1=\"3\" y1=\"12\" x2=\"3\" y2=\"12\"></line><line x1=\"3\" y1=\"18\" x2=\"3\" y2=\"18\"></line></svg>\n              </div>\n            </div>\n            \n            <!-- All notes -->\n            <Notes :notes=\"notesFilter\" :grid=\"grid\" @remove=\"removeNote\"/>\n          </div>\n        </section>\n\n      </div>\n    </div>\n</template>\n\n<script>\nimport Message from '@/components/Message.vue'\nimport NewNote from '@/components/NewNote.vue'\nimport Notes from '@/components/Notes.vue'\nimport Search from '@/components/Search.vue'\n\nexport default {\n  components: {\n    Message,\n    NewNote,\n    Notes,\n    Search\n  },\n  name: 'App',\n  data () {\n    return {\n        title: \"Notes App\",\n        message: null,\n        search: \"\",\n        grid: true,\n        note: {\n          title: \"\",\n          description: \"\"\n        },\n        notes: [\n        {\n          title: \"First note\",\n          description: \"The first note\",\n          date: new Date(Date.now()).toLocaleString()\n        },\n        {\n          title: \"Second note\",\n          description: \"The second note\",\n          date: new Date(Date.now()).toLocaleString()\n        },\n        {\n          title: \"Third note\",\n          description: \"The third note\",\n          date: new Date(Date.now()).toLocaleString()\n        }\n        ]\n    }\n  },\n      methods: {\n        addNote() {\n          let {title, description} = this.note;\n          if (title === '' || description === '') {\n            this.message = \"Input fields can't be empty\";\n            return false;\n          }\n          this.notes.push({\n            title,\n            description,\n            date: new Date(Date.now()).toLocaleString()\n          })\n          this.message = null;\n          this.note.title = \"\";\n          this.note.description = \"\";\n          },\n          removeNote (index) {\n            this.notes.splice(index, 1)\n          }\n        },\n        computed: {\n          notesFilter () {\n            let array = this.notes,\n                search = this.search\n            if (!search) return array\n            //to lower case\n            search = search.trim().toLowerCase()\n            //filter\n            array = array.filter(function (item) {\n              if (item.title.toLowerCase().indexOf(search) !== -1) {\n                return item\n              }\n            })\n            //Error\n            return array\n          }\n        }\n      }\n</script>\n\n<style lang=\"scss\">\n\n</style>\n"],"sourceRoot":"src"}]}